    },
    "homepage": "https://github.com/babyalexa/ecommerce-server#readme",
    "devDependencies": {
        "jest": "^25.1.0",
        "supertest": "^4.0.2"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "pg": "^7.18.1",
        "sequelize": "^5.21.4"
    }
}
